module Running_light
#(
parameter T=25'b1011111010111100001000000
)
(
input sys_clk,
input sys_rst_n,
output reg led0,
output reg led1,
output reg led2,
output reg led3
);
assign led={led3,led2,lde1,led0};
reg [24:0] cnt;
always @(posedge sys_clk or negedge sys_rst_n) begin
  if (!sys_rst_n) begin
    cnt<= 25'd0;
    led=4'b1110;
  end else begin
    if (cnt < T) begin
      cnt <= cnt + 1'b1;
    end else begin
      cnt<=25'd0;
      led<= (<<led)+1'b1;
    end
  end
end
endmodule